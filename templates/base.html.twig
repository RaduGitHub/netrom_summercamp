<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
              integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
              crossorigin="anonymous">
        {# {{ encore_entry_link_tags('app') }} #}
    {% endblock %}

    {% block javascripts %}
        {# {{ encore_entry_script_tags('app') }} #}
        {# <script type="text/javascript" src="../vendor/twitter/typeahead.js/dist/typeahead.jquery.min.js"></script> #}
        {# <script type="text/javascript" src="../vendor/twitter/typeahead.js/dist/bloodhound.min.js"></script> #}
        {# <script type="text/javascript" src="../vendor/twitter/typeahead.js/dist/typeahead.bundle.js"></script> #}
        <script type="text/javascript" src="../vendor/fortawesome/font-awesome/css/all.min.css"></script>
        <script type="text/javascript" src="../vendor/fortawesome/font-awesome/js/all.js"></script>
    {% endblock %}
</head>
<body>
{% block body %}
    <a href="{{ path('home') }}">Go to main page</a>
    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div class="alert alert-{{ label }}" role="alert">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}

{% endblock %}
</body>
</html>
